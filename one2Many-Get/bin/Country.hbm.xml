<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
 <class name="com.prestige.Country" table="country">
 <id name="cid" column="cid"></id>
 <property name="country_name" column="country_name"></property>
 <set name="Selection" cascade="all">
<key column="forevenid" />
<one-to-many class="com.prestige.State" />
 </set>
 </class>
 </hibernate-mapping>